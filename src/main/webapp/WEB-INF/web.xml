<web-app version="5.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-fragment_5_0.xsd">
  <display-name>DuEPublico</display-name>

  <absolute-ordering>
    <name>mir-layout</name>
    <name>mir-module</name>
    <name>mycore</name>
    <others/>
  </absolute-ordering>

  <servlet id="OAI4OER">
    <servlet-name>OAI4OER</servlet-name>
    <servlet-class>org.mycore.oai.MCROAIDataProvider</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OAI4OER</servlet-name>
    <url-pattern>/oer/oai</url-pattern>
  </servlet-mapping>

</web-app>